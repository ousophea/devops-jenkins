node {
  def remote = [:]
  remote.name = 'test'
  remote.host = '192.168.222.236'
  remote.user = 'gdi'
  remote.password = 'gdi@12345'
  remote.allowAnyHosts = true
  stage('Remote SSH') {
    sshCommand remote: remote, command: "cd /app/gdi_api_getway \n"
    sshCommand remote: remote, command: "ls"
  }
}