node {
  def remote = [:]
  remote.name = 'test'
  remote.host = '192.168.222.238'
  remote.user = 'gdi'
  remote.password = 'gdi@12345'
  remote.allowAnyHosts = true
  stage('Remote SSH') {
    sshCommand remote: remote, command: "cd /var/opt/app/gdi-core-deployment \n"
    sshCommand remote: remote, command: "sudo -S <<< \"gdi@12345\" sudo su \n"
    sshCommand remote: remote, command: "git pull \n"
  }
}