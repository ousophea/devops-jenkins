node {
  def remote = [:]
  remote.name = 'test'
  remote.host = '192.168.222.238'
  remote.user = 'gdi'
  remote.password = 'gdi@12345'
  remote.allowAnyHosts = true
  stage('Remote SSH') {
    sshCommand remote: remote, command: "cd /var/opt/app \n"
    sshCommand remote: remote, command: "ls \n"
    sshCommand remote: remote, command: "docker ps"
  }
}